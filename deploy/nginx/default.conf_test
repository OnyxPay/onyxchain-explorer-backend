server {
    listen 443 ssl default_server;
    listen [::]:443 ssl default_server;
    ssl_certificate /etc/letsencrypt/live/cepheus-explorer.onyxpay.co/fullchain.pem;
    ssl_certificate_key /etc/letsencrypt/live/cepheus-explorer.onyxpay.co/privkey.pem;
    server_name cepheus-explorer.onyxpay.co;

    location /api {
        proxy_pass http://localhost:8090;
    }

    location /v2 {
        proxy_pass http://ec2-18-218-87-55.us-east-2.compute.amazonaws.com:8080;
    }

    location /getAssetHolder {
        proxy_pass http://ec2-18-218-87-55.us-east-2.compute.amazonaws.com:8082;
    }

    error_log /var/log/nginx/error.log;
    access_log /var/log/nginx/access.log;
}
